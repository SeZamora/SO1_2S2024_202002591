#!/bin/bash
for i in {1..10}
do
        numero=$(tr -dc 1-4 </dev/urandom | head -c 1)
	if [ $numero -eq 1 ]; then
		docker run 
	elif [ $numero -eq 2 ]; then
                echo "opcion $numero"
	elif [ $numero -eq 3 ]; then
                echo "opcion $numero"
	else
                echo "opcion $numero"
	fi
done
